<p id="notice"><%= notice %></p>

<h1>Compares</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @compares.each do |compare| %>
      <tr>
        <td><%= link_to 'Show', compare %></td>
        <td><%= link_to 'Edit', edit_compare_path(compare) %></td>
        <td><%= link_to 'Destroy', compare, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Compare', new_compare_path %>
